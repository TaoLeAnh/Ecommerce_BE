using System.ComponentModel.DataAnnotations.Schema;

namespace Models
{
    [ComplexType]
    public class ProductOrderPK
    {
        [ForeignKey("Order")]
        public int OrderId { get; set; }
        public Order? Order { get; set; }

        [ForeignKey("Variant")]
        public int VariantId { get; set; }
        public Variant? Variant { get; set; }
    }
}
